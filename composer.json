{
  "name": "abuhawwa/textlocal",
  "description": "Textlocal SMS Channel for Laravel",
  "keywords": [
    "laravel",
    "notifications",
    "textlocal",
    "sms"
  ],
  "homepage": "https://github.com/abuhawwa/textlocal",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Abu Hawwa",
      "email": "avinash.haneefa@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.6|^7.1",
    "illuminate/notifications": "^5.8 || ^6.0 || ^7.0",
    "illuminate/support": "^5.8 || ^6.0 || ^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Abuhawwa\\Textlocal\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Abuhawwa\\Textlocal\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Abuhawwa\\Textlocal\\TextlocalServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
